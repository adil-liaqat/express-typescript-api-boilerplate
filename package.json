{
  "name": "typescript-express-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run build && concurrently --kill-others \"tsc -w -p src\" \"nodemon -r dotenv/config dist/index.js -e ts\"",
    "debug": "npm run build && concurrently --kill-others \"tsc -w -p src\" \"nodemon --inspect dist/index.js -e ts\"",
    "test:watch": "dotenv -e .env.test mocha -w ./test/**/*.ts --watch-extensions ts",
    "test": "dotenv -e .env.test mocha --exit --",
    "build": "tsc -p src",
    "start": "node -r dotenv/config dist/index.js",
    "build:docker": "docker build -t expressapi/express-typescript-starter .",
    "docker:compose": "docker-compose up",
    "format": "prettier --write 'src/**/*ts'",
    "lint": "eslint -c .eslintrc --ext .ts src/**/*.ts",
    "lint:fix": "eslint -c .eslintrc --fix --ext .ts src/**/*.ts",
    "lint:test": "eslint -c .eslintrc --fix --ext .ts test/**/*.ts",
    "format:test": "prettier --write 'test/**/*ts'",
    "coverage": "nyc npm test"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "celebrate": "^15.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "fresh": "^0.5.2",
    "glob": "^7.1.6",
    "helmet": "^4.2.0",
    "http-errors": "^1.8.0",
    "i18next": "^19.8.4",
    "i18next-fs-backend": "^1.0.7",
    "i18next-http-middleware": "^3.0.6",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "minimist": "^1.2.5",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "negotiator": "^0.6.2",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "sequelize": "^6.3.5",
    "serve-favicon": "^2.5.0",
    "swagger-jsdoc": "^6.0.0-rc.3",
    "swagger-ui-express": "^4.1.5",
    "tslint-config-prettier": "^1.18.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.4.0",
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.3.3",
    "@types/chai-subset": "^1.3.3",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.8",
    "@types/deep-equal-in-any-order": "^1.0.1",
    "@types/dirty-chai": "^2.0.2",
    "@types/ejs": "^3.0.5",
    "@types/express": "^4.17.9",
    "@types/glob": "^7.1.3",
    "@types/http-errors": "^1.8.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.161",
    "@types/mocha": "^5.2.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.18.26",
    "@types/nodemailer": "^6.4.0",
    "@types/nodemailer-smtp-transport": "^2.7.4",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/sequelize": "^4.28.9",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.8",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "chai-subset": "^1.6.0",
    "concurrently": "^3.5.0",
    "deep-equal-in-any-order": "^2.0.0",
    "dirty-chai": "^2.0.1",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.22.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-no-only-tests": "^3.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "mocha": "^10.0.0",
    "node-mocks-http": "^1.11.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "sinon": "^14.0.0",
