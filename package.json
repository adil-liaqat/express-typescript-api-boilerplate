{
  "name": "typescript-express-api",
  "version": "1.0.8",
  "private": true,
  "scripts": {
    "dev": "npm run build && concurrently --kill-others \"tsc -w -p src\" \"nodemon -r dotenv/config dist/index.js -e ts\"",
    "debug": "npm run build && concurrently --kill-others \"tsc -w -p src\" \"nodemon --inspect dist/index.js -e ts\"",
    "test:watch": "mocha -r ts-node/register -w ./spec/**/*.spec.ts --watch-extensions ts",
    "test": "mocha -r ts-node/register -w ./spec/**/*.spec.ts",
    "build": "tsc -p src",
    "start": "node -r dotenv/config dist/index.js",
    "build:docker": "docker build -t expressapi/express-typescript-starter .",
    "docker:compose": "docker-compose up",
    "format": "prettier --write 'src/**/*ts'",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:test": "tslint -c tslint.json 'spec/**/*.ts'",
    "format:test": "prettier --write 'spec/**/*ts'"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "fresh": "^0.5.2",
    "glob": "^7.1.6",
    "helmet": "^4.2.0",
    "http-errors": "^1.8.0",
    "i18next": "^19.8.4",
    "i18next-fs-backend": "^1.0.7",
    "i18next-http-middleware": "^3.0.6",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "minimist": "^1.2.5",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "negotiator": "^0.6.2",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "sequelize": "^6.3.5",
    "serve-favicon": "^2.5.0",
    "swagger-jsdoc": "^6.0.0-rc.3",
    "swagger-ui-express": "^4.1.5",
    "tslint-config-prettier": "^1.18.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.1.3",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.8",
    "@types/ejs": "^3.0.5",
    "@types/express": "^4.17.9",
    "@types/glob": "^7.1.3",
    "@types/http-errors": "^1.8.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.161",
    "@types/mocha": "^5.2.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.10",
    "@types/nodemailer": "^6.4.0",
    "@types/nodemailer-smtp-transport": "^2.7.4",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/sequelize": "^4.28.9",
    "@types/sinon": "^4.3.3",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "chai": "^4.1.2",
    "concurrently": "^3.5.0",
    "mocha": "^8.2.1",
    "node-mocks-http": "^1.9.0",
    "nodemon": "^2.0.7",
    "prettier": "^1.18.2",
    "sinon": "^5.0.7",
    "ts-node": "^6.0.3",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2"
  }
}
